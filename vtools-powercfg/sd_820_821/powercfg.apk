#!/system/bin/sh
# Project WIPE https://github.com/yc9559/cpufreq-interactive-opt
# Author: yc9559
# Generated at: Sat Apr 21 23:23:51 2018

# $1:value $2:file path
function set_value() {
	if [ -f $2 ]; then
		chown 0.0 $2
		chmod 0666 $2
		echo $1 > $2
		chmod 0444 $2
	fi
}

# $1:cpu0 $2:timer_rate $3:value
function set_param() {
	echo $3 > /sys/devices/system/cpu/$1/cpufreq/interactive/$2
}

action=$1
setenforce 0

# disable hotplug to switch governor
echo 0 > /sys/module/msm_thermal/core_control/enabled

set_value 1 /sys/devices/system/cpu/cpu2/online
chown 0.0 /sys/devices/system/cpu/cpu0/cpufreq/interactive/*
chown 0.0 /sys/devices/system/cpu/cpu2/cpufreq/interactive/*
chmod 0666 /sys/devices/system/cpu/cpu0/cpufreq/interactive/*
chmod 0666 /sys/devices/system/cpu/cpu2/cpufreq/interactive/*

# RunOnce
if [ ! -f /project_wipe_runonce ]; then
	# set flag
	echo 1 > /project_wipe_runonce

	# Perfd, nothing to worry about, if error the script will continue
	stop perfd
	if [ -f /data/system/perfd/default_values ]; then
		rm /data/system/perfd/default_values
	fi

	set_value 90 /proc/sys/kernel/sched_spill_load
	set_value 0 /proc/sys/kernel/sched_boost
	set_value 1 /proc/sys/kernel/sched_prefer_sync_wakee_to_waker
	set_value 40 /proc/sys/kernel/sched_init_task_load

	set_value 30 /proc/sys/kernel/sched_downmigrate
	set_value 45 /proc/sys/kernel/sched_upmigrate

	set_value 50 /sys/module/cpu_boost/parameters/input_boost_ms
	set_value 0 /sys/module/msm_performance/parameters/touchboost

	set_value 280000 /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
	set_value 280000 /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq

	# shared interactive parameters
	set_param cpu0 timer_rate 20000
	set_param cpu2 timer_rate 20000
	set_param cpu0 timer_slack 180000
	set_param cpu2 timer_slack 180000
	set_param cpu0 io_is_busy 0
	set_param cpu2 io_is_busy 0
	set_param cpu0 use_sched_load 1
	set_param cpu2 use_sched_load 1
	set_param cpu0 ignore_hispeed_on_notif 0
	set_param cpu2 ignore_hispeed_on_notif 0
	set_value 0 /sys/devices/system/cpu/cpu0/cpufreq/interactive/enable_prediction
	set_value 0 /sys/devices/system/cpu/cpu2/cpufreq/interactive/enable_prediction
fi

if [ "$action" = "powersave" ]; then
	set_value "0:980000 2:0" /sys/module/cpu_boost/parameters/input_boost_freq
	set_param cpu0 above_hispeed_delay "18000 1180000:58000 1580000:38000"
	set_param cpu0 hispeed_freq 980000
	set_param cpu0 go_hispeed_load 99
	set_param cpu0 boostpulse_duration 18000
	set_param cpu0 target_loads "85 380000:47 480000:2 580000:64 680000:21 780000:70 980000:80 1080000:99 1180000:93 1280000:71 1380000:96"
	set_param cpu0 min_sample_time 18000
	set_param cpu2 above_hispeed_delay "18000 1280000:58000 1780000:138000"
	set_param cpu2 hispeed_freq 1180000
	set_param cpu2 go_hispeed_load 99
	set_param cpu2 boostpulse_duration 18000
	set_param cpu2 target_loads "85 380000:35 480000:51 580000:64 680000:31 780000:70 880000:90 1080000:99 1280000:92 1380000:85 1480000:90 1580000:99"
	set_param cpu2 min_sample_time 18000
fi

if [ "$action" = "balance" ]; then
	set_value "0:1280000 2:0" /sys/module/cpu_boost/parameters/input_boost_freq
	set_param cpu0 above_hispeed_delay "58000 1580000:18000"
	set_param cpu0 hispeed_freq 1280000
	set_param cpu0 go_hispeed_load 97
	set_param cpu0 boostpulse_duration 18000
	set_param cpu0 target_loads "85 380000:49 480000:19 580000:64 680000:20 780000:70 1180000:99"
	set_param cpu0 min_sample_time 18000
	set_param cpu2 above_hispeed_delay "18000 1280000:58000 1380000:38000 1480000:58000 1780000:138000"
	set_param cpu2 hispeed_freq 1180000
	set_param cpu2 go_hispeed_load 98
	set_param cpu2 boostpulse_duration 18000
	set_param cpu2 target_loads "85 380000:47 480000:24 580000:64 680000:29 780000:70 980000:51 1080000:70 1180000:91 1380000:84 1580000:99"
	set_param cpu2 min_sample_time 18000
fi

if [ "$action" = "performance" ]; then
	set_value "0:1080000 2:0" /sys/module/cpu_boost/parameters/input_boost_freq
	set_param cpu0 above_hispeed_delay "18000 1180000:58000"
	set_param cpu0 hispeed_freq 1080000
	set_param cpu0 go_hispeed_load 91
	set_param cpu0 boostpulse_duration 78000
	set_param cpu0 target_loads "85 380000:47 480000:4 780000:64 880000:28 980000:42 1080000:66 1180000:84 1280000:99 1580000:90"
	set_param cpu0 min_sample_time 58000
	set_param cpu2 above_hispeed_delay "18000 1280000:38000 1380000:58000 1480000:38000 1580000:58000 1780000:38000"
	set_param cpu2 hispeed_freq 1180000
	set_param cpu2 go_hispeed_load 88
	set_param cpu2 boostpulse_duration 58000
	set_param cpu2 target_loads "85 380000:31 480000:52 580000:48 680000:5 780000:60 880000:72 980000:82 1080000:30 1180000:97 1280000:81 1380000:85 1480000:92 1580000:99"
	set_param cpu2 min_sample_time 18000
fi

if [ "$action" = "fast" ]; then
	# nothing
fi

chmod 0444 /sys/devices/system/cpu/cpu0/cpufreq/interactive/*
chmod 0444 /sys/devices/system/cpu/cpu2/cpufreq/interactive/*

# re-enable hotplug to switch governor
echo 1 > /sys/module/msm_thermal/core_control/enabled

exit 0